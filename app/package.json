{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@drizzle/react-components": "^1.5.1",
    "@drizzle/react-plugin": "^1.5.1",
    "@drizzle/store": "^1.5.1",
    "@ionic/react": "^4.11.8",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@types/qrcode.react": "^1.0.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "qrcode.react": "^1.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "jest": true,
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "react-app",
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "prefer-promise-reject-errors": [
        "off"
      ],
      "react/jsx-filename-extension": [
        "off"
      ],
      "react/prop-types": [
        "warn"
      ],
      "no-return-assign": [
        "off"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
